<%= form_with(model: @parent_category, local: true) do |form| %>
    <div>
      <%= form.label :parent_category %>
      <%= form.text_field :parent_category %>
    </div>
  
    <div>
      <%= form.label :icon %>
      <%= form.text_field :icon %>
    </div>
  
    <div>
      <%= form.label :color_code %>
      <%= form.text_field :color_code %>
    </div>
  
    <div>
      <%= form.submit %>
    </div>
  <% end %>
  